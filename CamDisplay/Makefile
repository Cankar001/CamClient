ifeq ($(config),debug)
  RESCOMP = windres
  TARGETDIR = bin/Debug-linux-x86_64/CamDisplay
  TARGET = $(TARGETDIR)/CamDisplay
  OBJDIR = bin-obj/Debug-linux-x86_64/CamDisplay
  DEFINES += -DCAM_PLATFORM_LINUX -DCAM_DEBUG
